<h1>Listing notes</h1>

<table>
  <tr>
  </tr>
  
<% for note in @notes %>
  <tr>
    <td><%= link_to 'Show', note_path(note) %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note_path(note), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New note', new_note_path %>